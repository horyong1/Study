package test1291;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int n1= sc.nextInt();
		int n2= sc.nextInt();
		sc.close();
		
		for(int i = 1; i<=9; i++) {
			for(int j = 0; j<2; j++) {
				System.out.printf("%d * %d = %2d   ",n1,i,n1*i);
				System.out.printf("%d * %d = %2d",n2,i,n2*i);
			}
		}
		
		
		
	}

}
