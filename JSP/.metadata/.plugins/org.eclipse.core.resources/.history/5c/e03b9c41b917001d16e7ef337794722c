package test1092;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		sc.close();
		
		int sum = 0;
		for(int i =0; i<y; i++) {
			sum = sum * x; 
		}
		sum = sum%20091024;
		
		System.out.println(sum);
	}

}
