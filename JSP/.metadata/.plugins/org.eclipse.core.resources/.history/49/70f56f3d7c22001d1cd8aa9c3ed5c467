package test1291;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n1,n2;
		
		while(true) {
			n1=sc.nextInt();
			n2=sc.nextInt();
			if(!(n1>1&&n1<10 || n1>1&&n1<10)) {
				System.out.println("INPUT ERROR!");
				
			}else {
				break;
			}
		}
		sc.close();
		
		
		for(int i = 1; i<=9; i++) {
			for(int j = 0; j<1; j++) {
				System.out.printf("%d * %d = %2d   ",n1,i,n1*i);
				System.out.printf("%d * %d = %2d",n2,i,n2*i);
			}
			System.out.println();
		}
		
		
		
	}

}
