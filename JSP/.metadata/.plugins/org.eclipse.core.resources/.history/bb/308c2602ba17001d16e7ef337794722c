package test1092;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		sc.close();
		
		if(x==0 || y==0) {
			System.out.println(1);
			System.exit(0);
		}
		
		int sum = x;
		for(int i =0; i<y-1; i++) {
			
			sum = sum * x; 
		}
		int sum2 = sum%20091024;
		
		System.out.println(sum);
	}

}
